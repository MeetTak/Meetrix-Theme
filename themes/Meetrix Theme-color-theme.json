{
    "name": "Meetrix",
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "activityBar.background": "#282C34",
        "activityBar.foreground": "#ABB2BF",
        "activityBarBadge.background": "#FFD700",
        "activityBarBadge.foreground": "#282C34",
        "badge.background": "#FFD700",
        "badge.foreground": "#282C34",
        "breadcrumb.activeSelectionForeground": "#D7BA97",
        "breadcrumb.background": "#282C34",
        "breadcrumb.focusForeground": "#FFFFFF",
        "breadcrumb.foreground": "#D7BA97",
        "breadcrumbPicker.background": "#282C34",
        "button.background": "#FFD700",
        "contrastBorder": "#33373B",
        "debugToolBar.background": "#282C34",
        "editor.background": "#282C34",
        "editor.foreground": "#ABB2BF",
        "editor.lineHighlightBackground": "#FFFFE0",
        "editor.selectionBackground": "#FFD70080", // Transparent yellow
        "editorCursor.foreground": "#ABB2BF",
        "editorWhitespace.foreground": "#ABB2BF",
        "sideBar.background": "#282C34",
        "sideBar.foreground": "#ABB2BF",
        "statusBar.background": "#282C34",
        "statusBar.foreground": "#ABB2BF",
        "tab.activeBackground": "#282C34",
        "tab.inactiveBackground": "#33373B",
        "tab.inactiveForeground": "#ABB2BF",
        "terminal.ansiBlack": "#282C34",
        "terminal.ansiBlue": "#81A1C1",
        "terminal.ansiBrightBlack": "#33373B",
        "terminal.ansiYellow": "#D7BA97"
    },
    "tokenColors": [
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#FFCC00"
            }
        },
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#BDB76B"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#FDF1BC"
            }
        },
        {
            "scope": ["variable", "entity.name.function"],
            "settings": {
                "foreground": "#81A1C1"
            }
        }
    ]
}